{
    "cluster": {
        "name": "jetty-test"
    },
    "http": {
        "enabled": true,
        "type": "com.sonian.elasticsearch.http.filter.FilterHttpServerTransportModule"
    },
    "sonian.elasticsearch.http.filter": {
        "__transport_type": "netty",
        "http_filter_chain": ["logging", "timeout"],
        "http_filter": {
            "timeout": {
                "type": "timeout"
            },
            "logging": {
                "type": "logging"
            }
        }
    }
}